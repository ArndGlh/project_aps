<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.epsi.projectaps.model">

    <class name="User" table="USER">
        <id name="id" column="ID">
            <generator class="native"/>
        </id>
        <property name="userName" type="StringType" column="USERNAME"/>
        <property name="password" type="StringType" column="PASSWORD"/>
        <property name="userMail" type="StringType" column="USERMAIL"/>
    </class>

</hibernate-mapping>